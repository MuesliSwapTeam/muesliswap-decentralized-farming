    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Server",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "muesliswap_onchain_staking.api.server:app",
                "--host",
                "localhost",
                "--port",
                "8008"
            ],
            "jinja": true
        },
        {
            "name": "Run Chain Querier",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.api.chain_querier"
        },
        {
            "name": "Create Farm",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.offchain.create_farm"
        },
        {
            "name": "Place Stake Order",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.offchain.place_stake_order"
        },
        {
            "name": "Cancel Stake Order",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.offchain.cancel_stake_order"
        },
        {
            "name": "Batch Stake Orders",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.offchain.batch_stake_orders"
        },
        {
            "name": "Place Unstake Order",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.offchain.place_unstake_order"
        },
        {
            "name": "Batch Unstake Orders",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.offchain.batch_unstake_orders"
        },
        {
            "name": "Build Contracts",
            "type": "debugpy",
            "request": "launch",
            "module": "muesliswap_onchain_staking.build"
        }
    ]
}